{
  "compilerOptions": {
    "baseUrl": ".",
    "inlineSourceMap": true,
    "inlineSources": true,
    "module": "ESNext",
    "target": "es6",
    "allowJs": true,
    "noImplicitAny": true,
    "moduleResolution": "node",
    "importHelpers": true,
    "types": [
      "node",
      "svelte"
    ],
    "lib": [
      "dom",
      "es5",
      "scripthost",
      "es2015"
    ],
    "allowSyntheticDefaultImports": true
  },
}
